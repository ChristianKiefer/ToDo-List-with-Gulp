{
  "env": {
    "browser": true,
    "node": true
  },
  "parserOptions": {
    "ecmaVersion": 2017
  },
  "extends": "eslint:recommended",
  "rules": {
    "consistent-return": 2,
    "indent": [ 1, 4 ],
    "semi": [ 1, "always" ],
    "space-unary-ops": 2,
    "no-console": "off",
    "no-unused-vars": "warn",
    "no-multi-spaces": "warn",
    "quotes": [ 1, "single" ]
  }
}